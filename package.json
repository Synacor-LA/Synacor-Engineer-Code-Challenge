{
  "name": "Synacor-Engineer-Code-Challenge",
  "version": "1.0.0",
  "description": "Welcome to the Synacor LA coding challenge! In this repo you'll find [instructions](Instructions.md) for the site we'd like you to create. We've tried to be conscious of your time, so this should be a fairly quick project.",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server --config ./webpack.config.js --mode development",
    "build": "webpack",
    "test": "jest",
    "updateSnaps": "jest --updateSnapshot",
    "test:watch": "jest --watch --verbose",
    "test:coverage": "jest --coverage --colors"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Synacor-LA/Synacor-Engineer-Code-Challenge.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Synacor-LA/Synacor-Engineer-Code-Challenge/issues"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "plugins": [
      [
        "@babel/plugin-proposal-class-properties"
      ]
    ]
  },
  "homepage": "https://github.com/Synacor-LA/Synacor-Engineer-Code-Challenge#readme",
  "dependencies": {
    "react": "^16.8.4",
    "react-dom": "^16.8.4"
  },
  "devDependencies": {
    "@babel/core": "^7.3.4",
    "@babel/plugin-proposal-class-properties": "^7.3.4",
    "@babel/polyfill": "^7.2.5",
    "@babel/preset-env": "^7.3.4",
    "@babel/preset-react": "^7.0.0",
    "babel-jest": "^24.3.1",
    "babel-loader": "^8.0.5",
    "css-loader": "^2.1.1",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.5.0",
    "enzyme-to-json": "^3.3.5",
    "fetch-mock": "^7.3.1",
    "jest": "^24.3.1",
    "node-fetch": "^2.3.0",
    "react-hot-loader": "^4.8.0",
    "style-loader": "^0.23.1",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.2.3",
    "webpack-dev-server": "^3.2.1"
  }
}
